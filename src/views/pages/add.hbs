<h1 class="text-center my-4">Nuevo Contacto</h1>
<form action="/add" method="post" class="w-25 mx-auto">
	<div class="mb-3">
		<label for="nombres">Nombres</label>
		<input type="text" name="nombres" class="form-control" required />
	</div>
	<div class="mb-3">
		<label for="ap_paterno">Apellido Paterno</label>
		<input type="text" name="ap_paterno" class="form-control" required />
	</div>
	<div class="mb-3">
		<label for="ap_materno">Apellido Materno</label>
		<input type="text" name="ap_materno" class="form-control" />
	</div>
	<div class="mb-3">
		<label for="t_fijo">Teléfono Fijo</label>
		<input type="text" name="t_fijo" class="form-control" />
	</div>
	<div class="mb-3">
		<label for="t_movil">Teléfono Móvil</label>
		<input type="text" name="t_movil" class="form-control" />
	</div>
	<div class="mb-3">
		<label for="email">Email</label>
		<input type="text" name="email" class="form-control" />
	</div>
	<input
		type="submit"
		value="Guardar Contacto"
		class="btn btn-primary btn-block"
	/>
</form>